#include<cstdio>
#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int i;double a[n],b[n],c[n];
    for(i=0;i<n;i++){
        cin>>a[i]>>b[i]>>c[i];
    }
    for(i=0;i<n;i++){
        if(c[i]-a[i]<b[i])
        {
            cout<<"Case #"<<i+1<<": true"<<endl;
        }
        else cout<<"Case #"<<i+1<<": false"<<endl;
    }
    return 0;

}
//要考虑到两数相加可能越界
